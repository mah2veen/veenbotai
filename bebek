local ScreenGui = Instance.new("ScreenGui")
local MAIN = Instance.new("Frame")
local Kill = Instance.new("TextButton")
local Teller = Instance.new("TextLabel")
local Close = Instance.new("TextButton")
 
--DO NOT EDIT BELOW THIS LINE OR THE SCRIPT MAY NOT WORK:
 
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
ScreenGui.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
 
MAIN.Name = "MAIN"
MAIN.Parent = ScreenGui
MAIN.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
MAIN.BorderColor3 = Color3.fromRGB(92, 109, 46)
MAIN.Position = UDim2.new(0.0356472805, 0, 0.328571439, 0)
MAIN.Size = UDim2.new(0, 204, 0, 269)
 
Kill.Name = "Kill"
Kill.Parent = MAIN
Kill.BackgroundColor3 = Color3.fromRGB(182, 0, 0)
Kill.Position = UDim2.new(0.142156869, 0, 0.423791826, 0)
Kill.Size = UDim2.new(0, 145, 0, 63)
Kill.Font = Enum.Font.PermanentMarker
Kill.Text = "oof'EM"
Kill.TextColor3 = Color3.fromRGB(255, 255, 255)
Kill.TextScaled = true
Kill.TextSize = 14.000
Kill.TextWrapped = true
kill.MauseButton1Down:connect(function()
    plr.Character.Humanoid:UnequipTools()
    local Humanoid = plr.Character.Humanoid:Clone()
    local Target = Player(Target.Text)
    local Tool = plr.Backpack:FindFirstChildOfClass("Tool")
 
    plr.Character.Animate.Disabled = true
    plr.Character.Humanoid:Destroy()
    Humanoid.Parent = plr.Character
    Tool.Parent = plr.Character
    game.Players.LocalPlayer.Character.Humanoid:ChangeState(15)
    firetouchinterest(Target.Character.HumanoidRootPart, Tool.Handle, 0);
end)
 
Teller.Name = "Teller"
Teller.Parent = MAIN
Teller.BackgroundColor3 = Color3.fromRGB(47, 172, 255)
Teller.Size = UDim2.new(0, 145, 0, 38)
Teller.Font = Enum.Font.Unknown
Teller.Text = "Blue's Brookhaven Kill GUI"
Teller.TextColor3 = Color3.fromRGB(255, 255, 255)
Teller.TextScaled = true
Teller.TextSize = 14.000
Teller.TextWrapped = true
 
Close.Name = "Close"
Close.Parent = MAIN
Close.Active = false
Close.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
Close.Position = UDim2.new(0.720588207, 0, 0, 0)
Close.Size = UDim2.new(0, 57, 0, 37)
Close.Font = Enum.Font.Creepster
Close.Text = "X"
Close.TextColor3 = Color3.fromRGB(255, 255, 255)
Close.TextScaled = true
Close.TextSize = 14.000
Close.TextWrapped = true
Close.MouseButton1Down:connect(function()
    MAIN.Visible = false
end)
